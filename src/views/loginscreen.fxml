<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="308.0" prefWidth="460.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="viewscontrollers.LoginscreenController">
   <children>
      <Label fx:id="titleLabel" alignment="CENTER" layoutX="91.0" layoutY="14.0" prefHeight="61.0" prefWidth="279.0" text="Scheduler Login" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="UsernameTextField" layoutX="131.0" layoutY="96.0" onAction="#UsernameTexFieldHandler" promptText="Username">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <TextField fx:id="PasswordTextField" layoutX="130.0" layoutY="144.0" onAction="#PasswordTextFieldHandler" promptText="Password">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <Button fx:id="LoginButton" layoutX="203.0" layoutY="192.0" mnemonicParsing="false" onAction="#LoggingButtonHandler" text="Login">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label fx:id="locationLabel" layoutX="29.0" layoutY="263.0" text="Location:">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
